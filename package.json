{
  "name": "daunus",
  "version": "0.2.2",
  "description": "Opinionated edge-first framework for TypeScript",
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --no-cache",
    "prettier": "prettier -w src",
    "lint": "ESLINT_USE_FLAT_CONFIG=false eslint src --ext .ts . && prettier -c src",
    "build": "unbuild",
    "dev": "unbuild --stub"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TaskWish/daunus.git"
  },
  "author": "Bersen Pajaziti",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TaskWish/daunus/issues"
  },
  "homepage": "https://www.daunus.com",
  "dependencies": {
    "@ai-sdk/provider": "^1.0.7",
    "@asteasolutions/zod-to-openapi": "^6.3.0",
    "@lukeed/uuid": "^2.0.1",
    "isomorphic-web-streams": "^0.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@ai-sdk/openai": "^1.1.9",
    "@types/jest": "^29.5.1",
    "eslint": "^9.18.0",
    "eslint-config-unjs": "^0.4.2",
    "eslint-plugin-unicorn": "^56.0.1",
    "jest": "^29.5.0",
    "prettier": "^3.4.2",
    "ts-jest": "^29.1.0",
    "typescript": "^5.3.2",
    "unbuild": "^2.0.0"
  }
}
